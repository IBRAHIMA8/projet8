<%= form_with(model: Blog.new, local: true) do |form| %>
  <div class="blog_title">
    <%= form.label :title %>
    <%= form.text_field :title %><br>
  </div>
  <br>
  <div class="blog_content">
    <%= form.label :content %>
    <%= form.text_field :content %><br>
  </div>
  <br>
  <%= form.submit %>
<% end %>
